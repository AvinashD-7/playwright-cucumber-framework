{
  "name": "playwright-cucumber-framework",
  "scripts": {
  "test": "npx @cucumber/cucumber --require tests/step-definitions --require tests/hooks",
  "sanity": "npx @cucumber/cucumber --require tests/step-definitions --require tests/hooks --tags '@sanity'",
  "allure:generate": "allure generate reports/allure-results --clean",
  "allure:open": "allure open"
},
  "dependencies": {
    "@cucumber/cucumber": "^9.0.0",
    "playwright": "^1.44.0",
    "dotenv": "^16.0.3",
    "winston": "^3.8.2",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "allure-commandline": "^2.20.1",
    "babel-register": "^6.26.0"
  }
}